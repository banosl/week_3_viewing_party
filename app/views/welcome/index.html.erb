<% if current_user %>
    <h1><%= current_user.email %></h1>
    <%= button_to 'Log Out', '/logout', method: :patch %>
<% else %>
    <%= button_to "Log In", '/login', method: :get %>
    <%= button_to "Create New User", register_path, method: :get %>
<% end %>

<% if current_user %>
<section class='existing-users'>
    <p>Existing Users: </p>
    <% @users.each do |user| %>
        <li><%= user.email %></li>
    <% end %>
</section>
<% end %>