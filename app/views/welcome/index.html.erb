<% if current_user %>
    <h1><%= current_user.email %></h1>
    <%= button_to 'Log Out'%>
<% else %>
    <%= button_to "Log In", '/login', method: :get %>
    <%= button_to "Create New User", register_path, method: :get %>
<% end %>

<section class='existing-users'>
    <p>Existing Users: </p>
    <% @users.each do |user| %>
        <li><%= link_to user.email, user_path(user.id) %></li>
    <% end %>
</section>